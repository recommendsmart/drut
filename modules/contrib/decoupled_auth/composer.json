{
  "name": "drupal/decoupled_auth",
  "description": "Allow decoupling of Drupal Authentication from Drupal Users.",
  "type": "drupal-module",
  "license": "GPL-2.0+",
  "require": {
    "drupal/core": "^8.3",
    "drupal/profile": "~1.0",
    "drupal/address": "~1.0"
  },
  "require-dev": {
    "drupal/coder": "^8.2",
    "drupal/user_registrationpassword": "~1.0-alpha3",
    "drupal/email_registration": "^1.0-rc7",
    "drupal/simplenews": "^1.0"
  },
  "conflict": {
    "drupal/email_registration": "<1.0-rc7"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\decoupled_auth\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Drupal\\Tests\\decoupled_auth\\": "tests/src"
    }
  }
}
