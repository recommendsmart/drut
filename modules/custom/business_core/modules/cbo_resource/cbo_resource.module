use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function cbo_resource_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'entity.resource.add_page':
      return '<p>' . t('Select the resource type for source add:') . '</p>';
  }
}
