<div class="contacts-row-image">
  {%  if image %}
    <a href="{{ url }}">{{ image }}</a>
  {% endif %}
</div>
<div class="contacts-row-main flex-fill">
  {% if label %}<{{ title_tag ?? "h3" }} class="contact-label"><a href="{{ url }}">{{ label }}</a></{{ title_tag ?? "h3" }}>{% endif %}
  {% if roles %}<small class="pull-right align-bottom contact-roles description">{{ roles }}</small>{% endif %}
</div>
